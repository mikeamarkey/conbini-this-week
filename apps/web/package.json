  "devDependencies": {
    "@types/node": "20.17.0",
    "@types/react": "18.3.12",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "autoprefixer": "^10.4.13",
    "eslint": "9.13.0",
    "eslint-config-next": "15.0.1",
    "eslint-config-prettier": "9.1.0",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.2.7",
    "typescript": "5.6.3"
  }
